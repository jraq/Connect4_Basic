{
  "extends": ["tslint:recommended", "tslint-react", "tslint-config-prettier"],
  "rules" : {
    "no-empty" : false,
    "no-empty-interface" : false,
    "ordered-imports" : false,
    "jsx-self-close" : false,
    "no-console" : false
  },
  "linterOptions": {
    "exclude": [
      "config/**/*.js",
      "node_modules/**/*.ts"
    ]
  }
}
